{
  "name": "smartathome",
  "version": "0.1.0",
  "homepage": "/",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@sentry/browser": "^6.12.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "axios": "^0.21.1",
    "axios-retry": "^3.1.9",
    "dvbjs": "^6.0.1",
    "gh-pages": "^3.2.3",
    "lodash-es": "^4.17.21",
    "moment-timezone": "^0.5.33",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "nuka-carousel": "^4.7.9",
    "react": "^18.0.0-alpha-1314299c7-20210901",
    "react-dom": "^18.0.0-alpha-1314299c7-20210901",
    "react-icons": "^4.2.0",
    "react-responsive-carousel": "^3.2.21",
    "react-router-dom": "^5.2.1",
    "react-scripts": "4.0.3",
    "react-toggle": "^4.1.2",
    "react-transition-group": "^4.4.2",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "scss": "node-sass src/sass/styles.scss -o src/styles/",
    "watch-scss": "node-sass --watch src/sass/styles.scss -o src/styles/",
    "build-scss": "npm run scss",
    "watch:style": "postcss src/styles/styles.css -o src/styles/main.css -w",
    "build:style": "postcss src/styles/styles.css -o src/styles/main.css",
    "watch-css": "npm-run-all -p watch-scss watch:style",
    "build-css": "npm-run-all -s build-scss build:style",
    "start-react": "react-scripts start",
    "start": "npm-run-all -p watch-css start-react",
    "build-react": "react-scripts build",
    "build": "npm-run-all -s build-css build-react",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "test": "react-scripts test --transformIgnorePatterns \"node_modules/(?!lodash-es)/\" --env=jsdom",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^6.2.0",
    "autoprefixer": "^9.8.6",
    "postcss": "^8.3.6",
    "postcss-cli": "^8.3.1",
    "react-test-renderer": "^17.0.1",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.2.9"
  }
}
